{
"name": "vegan-food",
"version": 2,
"builds":[
    {
        "src":"server.js",
        "use":"@now/node-server"}
],
"routes": [
    {
        "src": "/foods",
        "dest": "/server.js",
        "methods": ["GET","POST"]
      },
    {
        "src": "/foods/:foodId",
        "dest": "/server.js",
        "methods": ["GET"]
      },
      {
        "src": "/foods/:foodId",
        "dest": "/server.js",
        "methods": ["PUT"]
      },
      {
        "src": "/foods/:foodId",
        "dest": "/server.js",
        "methods": ["DELETE"]
      }
]
}